{
  "tasks": {
    "dev": "deno run -A https://deno.land/x/aleph@1.0.0-alpha.30/cli.ts dev",
    "start": "deno run -A https://deno.land/x/aleph@1.0.0-alpha.30/cli.ts start",
    "build": "deno run -A https://deno.land/x/aleph@1.0.0-alpha.30/cli.ts build",
    "store:example": "deno run --allow-env --allow-read=. --allow-net store/mod.ts"
  },
  "fmt": {
    "files": {
      "exclude": [
        "dist"
      ]
    }
  },
  "lint": {
    "files": {
      "exclude": [
        "dist",
        "vendor"
      ]
    }
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "dom.asynciterable",
      "deno.ns"
    ],
    "types": [
      "https://deno.land/x/aleph@1.0.0-alpha.30/types.d.ts",
      "./types.d.ts"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "https://esm.sh/react@18.1.0"
  },
  "importMap": "./import_map.json"
}